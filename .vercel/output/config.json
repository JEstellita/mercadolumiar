{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/health/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/mp-webhook/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/progresso/?$",
			"dest": "_render"
		},
		{
			"src": "^/apoie/?$",
			"dest": "_render"
		},
		{
			"src": "^/assinar/?$",
			"dest": "_render"
		},
		{
			"src": "^/produtores/?$",
			"dest": "_render"
		},
		{
			"src": "^/robots\\.txt/?$",
			"dest": "_render"
		},
		{
			"src": "^/sobre/?$",
			"dest": "_render"
		},
		{
			"src": "^/whatsapp/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}